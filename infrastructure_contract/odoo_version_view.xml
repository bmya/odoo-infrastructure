<?xml version="1.0" encoding="utf-8"?>
<!-- infrastructure.odoo_version -->
<openerp>
<data>


<!-- FORMVIEW -->
<record id="view_infrastructure_odoo_version_form" model="ir.ui.view">
    <field name="name">odoo_version.form</field>
    <field name="model">infrastructure.odoo_version</field>
    <field name="inherit_id" ref="infrastructure.view_infrastructure_odoo_version_form"/>
    <field name="arch" type="xml">
        <field name="default_branch_id" position="after">
            <field name="support_start_date"/>
            <field name="support_end_date"/>
            <field name="support_end_notification_date"/>
        </field>
    </field>
</record>

<!-- TREEVIEW -->
<record id="view_infrastructure_odoo_version_tree" model="ir.ui.view">
    <field name="name">infrastructure.odoo_version.tree</field>
    <field name="model">infrastructure.odoo_version</field>
    <field name="inherit_id" ref="infrastructure.view_infrastructure_odoo_version_tree"/>
    <field name="arch" type="xml">
        <field name="default_branch_id" position="after">
            <field name="support_start_date"/>
            <field name="support_end_date"/>
            <field name="support_end_notification_date"/>
        </field>
    </field>
</record>


</data>
</openerp>
